{
  "include": [
    "./libs/**/*.ts",
    "./libs/**/*.vue",
    "./target.config.ts"
  ],
  "exclude": ["node_modules", "dist", "out", "src/renderer/**/*"],
  "compilerOptions": {
    "noEmit": true,
    "noEmitOnError": true,
    "noUncheckedIndexedAccess": false,
    "noUnusedLocals": false,
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["DOM", "ESNext", "WebWorker"],
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "allowUnusedLabels": true,
    "isolatedModules": false,
    "composite": true,
    "removeComments": true,
    "noEmitHelpers": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "strict": true,
    "types": ["electron-vite/node", "vite/client"],
    "baseUrl": "."
  }
}
