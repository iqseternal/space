{
  "name": "space",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "suey",
  "homepage": "https://www.oupro.cn",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:app": "electron-vite dev --watch",
    "dev:scripts": "pnpm run -C scripts dev",
    "postinstall": "electron-builder install-app-deps",
    "build": "npm run typecheck && electron-vite build",
    "build:scripts": "pnpm run -C scripts build",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "build:all": "pnpm build:win && pnpm build:mac && pnpm build:linux",
    "prepare": "husky install"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@element-plus/icons-vue": "^2.1.0",
    "@formkit/auto-animate": "^0.8.0",
    "@suey/packages": "1.1.17",
    "@suey/pkg-utils": "^0.0.11",
    "@suey/pkg-web": "^0.0.3",
    "@suey/printer": "^1.1.5",
    "@suey/rocketry": "^0.0.4",
    "animate.css": "^4.1.1",
    "ant-design-vue": "4.x",
    "axios": "^1.5.0",
    "bufferutil": "^4.0.7",
    "dayjs": "^1.11.9",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.3.0",
    "fake-progress": "^1.0.4",
    "http": "0.0.1-security",
    "lodash": "^4.17.21",
    "mousetrap": "^1.6.5",
    "nightmare": "^3.0.2",
    "pako": "^2.1.0",
    "pinia": "^2.1.4",
    "selenium-webdriver": "^4.11.1",
    "utf-8-validate": "^6.0.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue3-menus": "^1.1.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@electron/remote": "^2.0.10",
    "@types/lodash": "^4.14.199",
    "@types/mousetrap": "^1.6.11",
    "@types/nightmare": "^2.10.6",
    "@types/node": "^18.16.19",
    "@types/pako": "^2.0.1",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "ajv": "^7",
    "crawlee": "^3.5.1",
    "depcheck": "^1.4.3",
    "electron": "^24.6.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1",
    "prettier": "^3.0.0",
    "puppeteer": "^20.9.0",
    "rollup-obfuscator": "^4.1.0",
    "simple-git": "^3.21.0",
    "terser": "^5.19.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.4"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": "eslint",
    "*.md": [
      "prettier --write"
    ]
  }
}
